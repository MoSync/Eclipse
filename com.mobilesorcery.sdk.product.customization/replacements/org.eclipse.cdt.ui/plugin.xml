            icon="icons/obj16/bin_obj.gif"
            id="org.eclipse.cdt.ui.binaryEditor"
            name="%DefaultBinaryFileEditor.name">
         <contentTypeBinding
               contentTypeId="org.eclipse.cdt.core.binaryFile">
         </contentTypeBinding>
      </editor>
   </extension>

